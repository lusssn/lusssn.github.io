---
layout: page
---
<!-- Main Content -->
<!-- <audio class="bg-music" src="" controls loop></audio> -->
<ul class="post-list">
    <% page.posts.each(function(post){ %>
    <li>
        <div class="post-preview">

            <h2 class="post-title">
                <%- post.title || "Untitled" %>
            </h2>
            <div class="post-subtitle">
                <%- post.subtitle || "" %>
            </div>
            <% if (post.excerpt){ %>
            <div class="post-content-preview">
                <%- post.excerpt %>
            </div>
            <% } %>
            <a href="<%- config.root %><%- post.path %>" target="_blank">READ MORE</a>

        </div>
    </li>
    <% }); %>
</ul>
<!-- Pager -->
<ul class="pager">
    <% if (page.prev){ %>
        <li class="previous">
            <a href="<%- config.root %><%- page.prev_link %>">&larr; Newer Posts</a>
        </li>
    <% } %>
    <% if (page.next){ %>
        <li class="next">
            <a href="<%- config.root %><%- page.next_link %>">Older Posts &rarr;</a>
        </li>
    <% } %>
</ul>

